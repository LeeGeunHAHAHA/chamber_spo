[global]
ioengine=libaio
direct=1
invalidate=1
norandommap=1
randrepeat=0
log_avg_msec=1000
group_reporting=1
time_based
thread=1

[job1]
filename=/dev/nvme0n1
description=4KRndWR_QD64TC1
readwrite=randrw
rwmixread=100
blocksize=4K
iodepth=16
numjobs=1
verify=pattern
verify_pattern=%o
do_verify=1
verify_fatal=1
runtime=10
