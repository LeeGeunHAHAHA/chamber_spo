[global]
readwrite=randrw
#rwmixwrite=0
rwmixwrite=50
#blocksize=4k
#blocksize_range=512-128k,512-128k
blocksize=8k,8k
blockalign=2k,2k
ioengine=libaio
numjobs=4
thread=1
direct=1
iodepth=128
iodepth_batch=16
iodepth_batch_complete=16
group_reporting=1
time_based=1
runtime=300
invalidate=1
norandommap=1
randrepeat=0
iopsavgtime=1000
size=1%

[fio_job]
filename=/dev/nvme0n1
