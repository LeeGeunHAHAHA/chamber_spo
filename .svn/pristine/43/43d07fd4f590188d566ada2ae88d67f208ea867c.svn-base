[global]
readwrite=randrw
rwmixwrite=100
#rwmixwrite=50
blocksize=4kb
blockalign=512
ioengine=libaio
numjobs=4
thread=1
direct=1
iodepth=128
runtime=7200
group_reporting=1
iodepth_batch=16
iodepth_batch_complete=16
time_based=1
invalidate=1
norandommap=1
randrepeat=0
iopsavgtime=1000

[fio_job]
filename=/dev/nvme0n1
